          }
        ]
      ]
    },
    "Upload to YouTube": {
      "main": [
        [
          {
            "node": "Success Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Success Response": {
      "main": [
        [
          {
            "node": "Respond to Webhook",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  },
  "staticData": null,
  "tags": [],
  "triggerCount": 1,
  "meta": {
    "description": "YouTube Video Dubbing Workflow - Downloads a YouTube video, transcribes it, translates to German, generates German TTS audio, overlays a static image to cover avatar, and uploads to YouTube.",
    "templateCredsSetupCompleted": false
  }
}
